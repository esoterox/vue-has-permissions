{
  "name": "vue-has-permissions",
  "description": "A Vue.js prototype function which checks whether or not a user has the permissions / role required for certain components to render or code to run",
  "author": "Ravi Pradhan (https://ravii.dev)",
  "main": "dist/vue-has-permissions.common.js",
  "version": "0.1.0",
  "private": false,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build --target lib --name vue-has-permissions ./src/plugin/hasPermissions.js",
    "lint": "vue-cli-service lint"
  },
  "keywords": [
    "vue",
    "permissions"
  ],
  "files": [
    "dist/*"
  ],
  "dependencies": {
    "core-js": "^2.6.5",
    "vue": "^2.6.10"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.11.0",
    "@vue/cli-plugin-eslint": "^3.11.0",
    "@vue/cli-service": "^3.11.0",
    "@vue/eslint-config-standard": "^4.0.0",
    "babel-eslint": "^10.0.1",
    "eslint": "^5.16.0",
    "eslint-plugin-vue": "^5.0.0",
    "vue-template-compiler": "^2.6.10"
  }
}
